<p id="notice"><%= notice %></p>

<h1>Part Numbers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sort</th>
      <th>Active status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @part_numbers.each do |part_number| %>
      <tr>
        <td><%= part_number.name %></td>
        <td><%= part_number.sort %></td>
        <td><%= part_number.active_status %></td>
        <td><%= link_to 'Show', part_number %></td>
        <td><%= link_to 'Edit', edit_part_number_path(part_number) %></td>
        <td><%= link_to 'Destroy', part_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Part Number', new_part_number_path %>
