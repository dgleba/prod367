
<row class="row is-table-row" >

  <div class="col-sm-1 ">
    <%= link_to 'Create', new_pp_parkingpass_path , class: "btn btn-success"%>
  </div>

  <div class="col-sm-2 ">
    <h4>Listing Pp Parkingpasses</h4>
  </div>

  <div class="col-sm-3">
    <%= search_form_for @q, class: 'search-form' do |f| %>
      <div class="input-group">
        <%= f.search_field :en_name_cont, class: 'form-control' %>
        <span class="input-group-btn">
          <button class="btn btn-primary">Search <i class="fa fa-search"></i>
          </button>
        </span>
      </div>
    <% end %>
  </div>

  <div class="col-sm-4 ">
    <%= paginate @pp_parkingpasses %>
  </div>
  
  <div class="col-sm-2 ">
    <%= page_entries_info @pp_parkingpasses, :entry_name => 'item' %>
  </div>
  
</row>


<table class="table table-striped">
  <thead>
    <tr>
      <th colspan="1"></th>
            <th>En name</th>
      <th>Parking pass</th>
      <th>Dept</th>
      <th>Company</th>
      <th>Grouping</th>
      <th>Estatus</th>
      <th>Supervisor</th>
      <th>En clock</th>
    </tr>
  </thead>

  <tbody>
    <% @pp_parkingpasses.each do |pp_parkingpass| %>
      <tr>
        <td>
          <div class="btn-group">
          <%= link_to 'Show', pp_parkingpass, class: "btn btn-info", :style=>'color:#FFFFFF;' %>
          <%= link_to 'Edit', edit_pp_parkingpass_path(pp_parkingpass), class: "btn btn-primary", :style=>'color:#FFFFFF;' %>
          <%= link_to 'Destroy', pp_parkingpass, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-warning", :style=>'color:#FFFFFF;' %>
          </div>
        </td>
        
        <td><%= pp_parkingpass.en_name %></td>
        <td><%= pp_parkingpass.parking_pass %></td>
        <td><%= pp_parkingpass.dept %></td>
        <td><%= pp_parkingpass.company %></td>
        <td><%= pp_parkingpass.grouping %></td>
        <td><%= pp_parkingpass.estatus %></td>
        <td><%= pp_parkingpass.supervisor %></td>
        <td><%= pp_parkingpass.en_clock %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
